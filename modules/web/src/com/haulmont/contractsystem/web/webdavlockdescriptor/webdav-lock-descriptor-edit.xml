<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.haulmont.contractsystem.web.webdavlockdescriptor.WebdavLockDescriptorEdit"
        datasource="webdavLockDescriptorDs"
        focusComponent="fieldGroup"
        messagesPack="com.haulmont.contractsystem.web.webdavlockdescriptor">
    <dsContext>
        <datasource id="webdavLockDescriptorDs"
                    class="com.haulmont.webdav.entity.WebdavLockDescriptor"
                    view="webdavLockDescriptor-with-deps-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="webdavLockDescriptorDs">
            <column width="250px">
                <field property="timeout"/>
                <field property="webdavDocument"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>

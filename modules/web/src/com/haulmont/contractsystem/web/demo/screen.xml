<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.haulmont.contractsystem.web.demo.Screen"
        messagesPack="com.haulmont.contractsystem.web.demo">
    <dsContext>
        <collectionDatasource id="webdavDocumentVersionsDs"
                              class="com.haulmont.webdav.entity.WebdavDocumentVersion"
                              view="webdavDocumentVersion-security">
            <query>
                <![CDATA[select e from webdav$WebdavDocumentVersion e]]>
            </query>
        </collectionDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout>
        <dataGrid id="data"
                  datasource="webdavDocumentVersionsDs"
                  height="100px"
                  width="200px">
            <columns>
                <column property="contentType"/>
                <column property="sha1sum"/>
                <column property="naturalVersionId"/>
            </columns>
        </dataGrid>
    </layout>
</window>

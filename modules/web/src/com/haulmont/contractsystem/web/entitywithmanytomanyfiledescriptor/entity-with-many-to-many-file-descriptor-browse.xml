<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="com.haulmont.contractsystem.web.entitywithmanytomanyfiledescriptor.EntityWithManyToManyFileDescriptorBrowse"
        focusComponent="entityWithManyToManyFileDescriptorsTable"
        lookupComponent="entityWithManyToManyFileDescriptorsTable"
        messagesPack="com.haulmont.contractsystem.web.entitywithmanytomanyfiledescriptor">
    <dsContext>
        <groupDatasource id="entityWithManyToManyFileDescriptorsDs"
                         class="com.haulmont.contractsystem.entity.EntityWithManyToManyFileDescriptor"
                         view="entityWithManyToManyFileDescriptor-view">
            <query>
                <![CDATA[select e from contractsystem$EntityWithManyToManyFileDescriptor e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="entityWithManyToManyFileDescriptorsTable"
            spacing="true">
        <filter id="filter"
                applyTo="entityWithManyToManyFileDescriptorsTable"
                datasource="entityWithManyToManyFileDescriptorsDs">
            <properties include=".*"/>
        </filter>
        <groupTable id="entityWithManyToManyFileDescriptorsTable"
                    width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
            </actions>
            <columns>
                <column id="name"/>
            </columns>
            <rows datasource="entityWithManyToManyFileDescriptorsDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="entityWithManyToManyFileDescriptorsTable.create"/>
                <button id="editBtn"
                        action="entityWithManyToManyFileDescriptorsTable.edit"/>
                <button id="removeBtn"
                        action="entityWithManyToManyFileDescriptorsTable.remove"/>
            </buttonsPanel>
        </groupTable>
    </layout>
</window>

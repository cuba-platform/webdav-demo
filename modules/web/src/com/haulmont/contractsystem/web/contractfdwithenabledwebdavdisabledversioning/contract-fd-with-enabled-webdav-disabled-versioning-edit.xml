<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.haulmont.contractsystem.web.contractfdwithenabledwebdavdisabledversioning.ContractFDWithEnabledWebdavDisabledVersioningEdit"
        datasource="contractFDWithEnabledWebdavDisabledVersioningDs"
        focusComponent="fieldGroup"
        messagesPack="com.haulmont.contractsystem.web.contractfdwithenabledwebdavdisabledversioning">
    <dsContext>
        <datasource id="contractFDWithEnabledWebdavDisabledVersioningDs"
                    class="com.haulmont.contractsystem.entity.ContractFDWithEnabledWebdavDisabledVersioning"
                    view="contractFDWithEnabledWebdavDisabledVersioning-default-view"/>
        <collectionDatasource id="contractorsDs"
                              class="com.haulmont.contractsystem.entity.ContractUser"
                              view="_minimal">
            <query>
                <![CDATA[select e from contractsystem$ContractUser e]]>
            </query>
        </collectionDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="contractFDWithEnabledWebdavDisabledVersioningDs">
            <column width="250px">
                <field property="number"/>
                <field optionsDatasource="contractorsDs"
                       property="contractor"/>
                <field property="document"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>

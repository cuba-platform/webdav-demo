<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.haulmont.contractsystem.web.webdavdocumentversion.WebdavDocumentVersionEdit"
        datasource="webdavDocumentVersionDs"
        focusComponent="fieldGroup"
        messagesPack="com.haulmont.contractsystem.web.webdavdocumentversion">
    <dsContext>
        <datasource id="webdavDocumentVersionDs"
                    class="com.haulmont.webdav.entity.WebdavDocumentVersion"
                    view="webdavDocumentVersion-with-deps-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="webdavDocumentVersionDs">
            <column width="250px">
                <field property="fileDescriptor"/>
                <field property="webdavDocument"/>
                <field property="contentType"/>
                <field property="contentLength"/>
                <field property="sha1sum"/>
                <field property="naturalVersionIdentifier"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>

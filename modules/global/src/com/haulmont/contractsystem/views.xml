<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.haulmont.contractsystem.entity.Contract"
          extends="_local"
          name="contract-with-deps-view">
        <property name="documents"
                  view="_minimal">
        </property>
        <property name="contractor"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.contractsystem.entity.Contract"
          extends="_local"
          name="contract-with-contractor-view">
        <property name="contractor"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.contractsystem.entity.Contractor"
          extends="_local"
          name="contractor-view">
        <property name="contract"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.webdav.entity.WebdavDocument"
          extends="_local"
          name="webdavDocument-view-with-dependencies">
        <property name="versions"
                  view="_minimal">
            <property name="fileDescriptor"
                      view="_minimal">
                <property name="size"/>
            </property>
            <property name="webdavDocument"
                      view="_minimal"/>
            <property name="contentType"/>
            <property name="contentLength"/>
            <property name="sha1sum"/>
            <property name="naturalVersionIdentifier"/>
        </property>
        <property name="webdavLockDescriptor"
                  view="_minimal">
            <property name="timeout"/>
            <property name="webdavDocument"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.haulmont.webdav.entity.WebdavDocumentVersion"
          extends="_local"
          name="webdavDocumentVersion-with-deps-view">
        <property name="fileDescriptor"
                  view="_minimal"/>
        <property name="webdavDocument"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.webdav.entity.WebdavLockDescriptor"
          extends="_local"
          name="webdavLockDescriptor-with-deps-view">
        <property name="webdavDocument"
                  view="_minimal"/>
    </view>
</views>
